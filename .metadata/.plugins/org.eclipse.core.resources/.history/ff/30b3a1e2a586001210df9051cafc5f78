package com.fer.pasajero.receivers;

import android.util.Log;

import com.fer.smsc.broadcast.SmsReceiver;
import com.fer.smsc.messageProtocol.sender.MessageReceiver;

public class SmsBroadCastReceiver extends SmsReceiver {

	
	public SmsBroadCastReceiver() {
		setMessageReceiver(new MessageReceiver() {
			
			@Override
			public void onMessageReceived(String sender, String message) {
				Log.d("SMS_Receiver", sender+": "+message);
			}
		});
	}
}
