package com.ft.clientetaxi;

import android.content.Context;
import android.content.Intent;

import com.fer.smsc.messageProtocol.sender.MessageReceiver;

public class SmsReceiver extends com.fer.smsc.broadcast.SmsReceiver{

	public SmsReceiver() {
		setMessageReceiver(new MessageReceiver() {
			
			@Override
			public void onMessageReceived(String sender, String message, Context context) {
				Intent i = new Intent(context, ConfirmarServicioActivity.class);
				
			}
		});
	}
}
