package com.fer.pasajero;

import android.app.Application;
import android.content.IntentFilter;

public class PasajeroApplication extends Application{

	PasajeroManager pasajeroManager;
	
	public PasajeroApplication() {
		pasajeroManager = new PasajeroManager();
		IntentFilter filter = new IntentFilter();
		registerReceiver(new SmsBroadCastReceiver(), filter)
	}
	
	public PasajeroManager getPasajeroManager()
	{
		return pasajeroManager;
	}
}
